    <servlet-mapping>
        <servlet-name>JointEre</servlet-name>
        <url-pattern>/JointEre</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ENIE</servlet-name>
        <url-pattern>/ENIE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Merge</servlet-name>
        <url-pattern>/Merge</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Visualize</servlet-name>
        <url-pattern>/Visualize</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <listener>
        <listener-class>com.artistech.ee.web.PipelineInit</listener-class>
    </listener>
</web-app>
