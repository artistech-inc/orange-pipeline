    </servlet>
    <servlet>
        <servlet-name>Merge</servlet-name>
        <servlet-class>com.artistech.ee.green.Merge</servlet-class>
        <init-param>
            <param-name>path</param-name>
            <param-value>/work/Documents/FOUO/EntityExtraction/joint_ere</param-value>
        </init-param>
        <init-param>
            <param-name>classpath</param-name>
            <param-value>ere-11-08-2016_small.jar:lib/commons-io-2.4.jar:lib/commons-lang3-3.1.jar:lib/commons-math3-3.2.jar:lib/dom4j-1.6.1.jar:lib/edu.mit.jwi_2.2.3_jdk.jar:lib/foo:lib/grmm-deps.jar:lib/jaxen-1.1-beta-6.jar:lib/mallet-deps.jar:lib/mallet.jar:lib/opennlp-maxent-3.0.2-incubating.jar:lib/opennlp-tools-1.5.2-incubating.jar:lib/RadixTree-0.3.jar:lib/stanford-corenlp-3.3.1.jar:lib/stanford-corenlp-3.3.1-models.jar:lib/trove-3.0.3.jar</param-value>
        </init-param>
